{
  "name": "uba-bootstrap-theme",
  "version": "0.1.6",
  "description": "UBA branded Bootstrap 4 theme",
  "scripts": {
    "dist": "./scripts/dist",
    "start": "npm-run-all --parallel server-watch compile-watch",
    "server-watch": "live-server ./documentation",
    "compile": "faucet",
    "compile-watch": "yarn run compile --watch --config=faucet.config.dev.js",
    "install-docs": "./scripts/docs",
    "test": "sass-lint -v -q",
    "prepare": "faucet"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/innoq/uba-bootstrap-theme.git"
  },
  "keywords": [
    "UBA",
    "Bootstrap",
    "Theme"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/innoq/uba-bootstrap-theme/issues"
  },
  "homepage": "https://github.com/innoq/uba-bootstrap-theme#readme",
  "style": "dist/css/uba-bootstrap-theme.css",
  "sass": "src/styles/index.scss",
  "devDependencies": {
    "faucet-pipeline-sass": "^1.2.0",
    "faucet-pipeline-static": "^1.0.0",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.2",
    "sass-lint": "^1.12.1"
  },
  "dependencies": {

  }
}
